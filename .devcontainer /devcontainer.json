{
  "customizations": {
    "vscode": {
      "extensions": [
        "james-yu.latex-workshop"
      ],
      "settings": {
        "latex-workshop.view.pdf.viewer": "tab",
        "files.associations": {
          "*.tex": "latex"
        },
        "[latex]": {
          "editor.defaultFormatter": "James-Yu.latex-workshop"
        },
        "workbench.editorAssociations": {
          "*.md": "vscode.markdown.preview.editor"
        }
      }
    }
  }
}